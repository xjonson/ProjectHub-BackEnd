{
  "version": 2,
  "builds": [
    { "src": "client/*", "use": "@now/static" },
    { "src": "client/assets/css/**", "use": "@now/static" },
    { "src": "client/assets/image/**", "use": "@now/static" },
    { "src": "client/assets/material-icons/**", "use": "@now/static" },
    { "src": "app.js", "use": "@now/node" },
    { "src": "app/model/*.js", "use": "@now/node" },
    { "src": "app/routes/api/*.js", "use": "@now/node" },
    { "src": "app/utils/*.js", "use": "@now/node" },
    { "src": "app/validation/*.js", "use": "@now/node" },
    { "src": "config/*.js", "use": "@now/node" }
  ]
}